{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
      "whitespace": false,
      "no-inferrable-types": false,
      "quotemark": false,
      "no-trailing-whitespace": false,
      "one-line": false,
      "prefer-const": false,
      "no-shadowed-variable": false
    }
}
